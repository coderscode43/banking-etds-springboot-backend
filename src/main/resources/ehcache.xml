<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">

	<diskStore path="java.io.tmpdir" />


	<cache name="clientDetail" maxEntriesLocalHeap="20"
		eternal="true">
		<persistence strategy="localTempSwap" />
	</cache>
	<cache name="employeesInGroup" maxEntriesLocalHeap="20"
		eternal="true">
		<persistence strategy="localTempSwap" />
	</cache>
	<cache name="impDates" maxEntriesLocalHeap="20" eternal="true">
		<persistence strategy="localTempSwap" />
	</cache>
	<cache name="recentNotifications" maxEntriesLocalHeap="20"
		eternal="true">
		<persistence strategy="localTempSwap" />
	</cache>

	<cache name="recentRemark" maxEntriesLocalHeap="20"
		eternal="true">
		<persistence strategy="localTempSwap" />
	</cache>
	
	<cache name="login" maxEntriesLocalHeap="20"
		eternal="true">
		<persistence strategy="localTempSwap" />
	</cache>
		<cache name="employeeInchargeGroupCode" maxEntriesLocalHeap="20"
		eternal="true">
		<persistence strategy="localTempSwap" />
	</cache>
	<cache name="employeeAdmin" maxEntriesLocalHeap="20"
		eternal="true">
		<persistence strategy="localTempSwap" />
	</cache>
	

</ehcache>